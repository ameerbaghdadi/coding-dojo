package ZooKeeper2;

public class Gorilla extends Mammal{
	public void throwSomething() {
		System.out.println("----------- eat Bananas -----------");
		System.out.println("the gorilla has thrown something and decrease the energy level by 5");
		this.setEnergyLevel(this.getEnergyLevel()-5);
		System.out.println("the gorilla energy level is "+this.getEnergyLevel());
	}
	
	public void eatBananas() {
		System.out.println("----------- eat Bananas -----------");
		System.out.println("the gorilla has eat Bananas and increase its energy by 10");
		this.setEnergyLevel(this.getEnergyLevel()+10);
		System.out.println("the gorilla energy level is "+this.getEnergyLevel());
	}
	
	public void climb() {
		System.out.println("----------- climb -----------");
		System.out.println("the gorilla has climbed a tree and decrease its energy by 10");
		this.setEnergyLevel(this.getEnergyLevel()+10);
		System.out.println("the gorilla energy level is "+this.getEnergyLevel());
	}
}
