package ZooKeeper2;

public class BatTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Bat b = new Bat();
		b.setEnergyLevel(300);
		b.fly();
		b.eatHumans();
		b.attackTown();
		
		Gorilla g = new Gorilla();
		g.setEnergyLevel(100);
		g.throwSomething();
		g.eatBananas();
		g.climb();
	}
}